  <DataSets>
    <DataSet Name="DemandDSet">
      <Query>
        <DataSourceName>DemandDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@DemandId">
            <Value>=Parameters!DemandId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select         d.DemandDate,d.DemandNo,d.Remarks,p.ProductName,p.ProductCode,di.Qty,
               (select Name from Company where CompanyId=d.CompanyId) as Company,
               (select Url from UrlInfo where UrlType=1)+'Images/Logo/'+(select CompanyLogo from Company where CompanyId=d.CompanyId) as CompanyLogo,
               (select [Address] from Company where CompanyId=d.CompanyId) as CompanyAddress,
               (select Phone from Company where CompanyId=d.CompanyId) as CompanyPhone,
               (select Email from Company where CompanyId=d.CompanyId) as CompanyEmail
from           Erp.DemandItem di 
inner join     Erp.Demand d on di.DemandId=d.DemandId
inner join     Erp.Product p on di.ProductId=p.ProductId
where          d.DemandId=@DemandId</CommandText>
      </Query>
