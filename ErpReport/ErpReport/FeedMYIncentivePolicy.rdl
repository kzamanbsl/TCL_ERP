  <DataSets>
    <DataSet Name="ProfitLossNoteDSet">
      <Query>
        <DataSourceName>ProfitLossNotDS</DataSourceName>
        <CommandText>Select id.IncentiveId, id.MinQty, id.MaxQty,id.Rate,i.CompanyId,i.IncentiveType from Erp.IncentiveDetail id
join Erp.Incentive i on id.IncentiveId = i.IncentiveId
where id.IsActive = 1 and i.CompanyId = 8</CommandText>
      </Query>
