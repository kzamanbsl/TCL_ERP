        <CommandText>   
   DECLARE @FromDate DATE
   SELECT @FromDate = CONVERT(VARCHAR, CONVERT(DATE, @StrFromDate, 105)) 
   
   select       h3.Id as IdH3,h3.AccName as AccNameH3,
				h3.OrderNo as OrderNoH3,
                gl.Id as IdGL,gl.ParentId as ParentIdGL,gl.AccName as AccNameGL,
				sum(isnull(vd.DebitAmount,0) - isnull(vd.CreditAmount,0)) as Balance

   from           Head2 h2
   inner join   Head3 h3 on h2.Id=h3.ParentId
   inner join   Head4 h4 on h3.Id=h4.ParentId
   inner join   Head5 h5 on h4.Id=h5.ParentId
