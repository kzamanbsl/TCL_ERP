  <DataSets>
    <DataSet Name="FixedAssetsDSet">
      <Query>
        <DataSourceName>BalanceSheetNoteDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StrFromDate">
            <Value>=Parameters!StrFromDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CompanyId">
            <Value>=Parameters!CompanyId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>   DECLARE @FromDate DATE
   SELECT @FromDate = CONVERT(VARCHAR, CONVERT(DATE, @StrFromDate, 105)) 
   
   select       h2.Id as IdH2,h2.AccName as AccNameH2,
                h2.OrderNo as OrderNoH2,
                gl.Id as IdGL,gl.ParentId as ParentIdGL,gl.AccName as AccNameGL,
				sum(isnull(vd.DebitAmount,0) - isnull(vd.CreditAmount,0)) as Balance

   from         Head2 h2
   inner join   Head3 h3 on h2.Id=h3.ParentId
   inner join   Head4 h4 on h3.Id=h4.ParentId
   inner join   Head5 h5 on h4.Id=h5.ParentId
   inner join   HeadGL gl on H5.Id=gl.ParentId
   inner join   VoucherDetail vd on gl.Id=vd.AccountHeadId
   inner join   Voucher v on vd.VoucherId=v.VoucherId
   where        h2.CompanyId = @CompanyId and v.VoucherDate&lt;=@FromDate and 
   v.IsActive =1
   AND v.IsSubmit =1
   AND vd.IsActive = 1
   AND
   v.CompanyId=@CompanyId and h2.AccCode like '1%' and lower(h2.AccName)&lt;&gt;lower('CURRENT ASSETS')

   group by     h2.Id,h2.AccName,h2.OrderNo,gl.Id,gl.ParentId,gl.AccName
   having       sum(isnull(vd.CreditAmount,0) - isnull(vd.DebitAmount,0))&lt;&gt;0
   order by     h2.OrderNo</CommandText>
      </Query>
