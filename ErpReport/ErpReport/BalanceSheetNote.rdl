    and  v.VoucherDate&lt;=@FromDate 
    AND v.IsActive = 1
    AND v.IsSubmit =1
    AND vd.IsActive =1
    and v.CompanyId=@CompanyId 
    and lower(h2.AccName)=lower('CURRENT ASSETS')

   group by     h3.Id,h3.AccName,h3.OrderNo,gl.Id,gl.ParentId,gl.AccName
   having       sum(isnull(vd.CreditAmount,0) - isnull(vd.DebitAmount,0))&lt;&gt;0
   order by     h3.OrderNo
