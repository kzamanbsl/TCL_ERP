    <DataSet Name="CompanyDSet">
      <Query>
        <DataSourceName>BalanceSheetDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@CompanyId">
            <Value>=Parameters!CompanyId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        Name, Address,
                             (SELECT        Url
                               FROM            UrlInfo
                               WHERE        (UrlType = 1)) + 'Images/Logo/' + CompanyLogo AS CompanyLogo
FROM            Company
WHERE        (CompanyId = @CompanyId)</CommandText>
      </Query>
      <Fields>
        <Field Name="Name">
          <DataField>Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Address">
          <DataField>Address</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CompanyLogo">
          <DataField>CompanyLogo</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="AssetDSet">
      <Query>
        <DataSourceName>BalanceSheetDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StrFromDate">
            <Value>=Parameters!StrFromDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CompanyId">
            <Value>=Parameters!CompanyId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText> DECLARE @FromDate DATE
    SELECT @FromDate = CONVERT(VARCHAR, CONVERT(DATE, @StrFromDate, 105))

	SELECT * FROM (SELECT Head2.Id, 
	                      Head2.AccName as AccName, 
						  SUM(VoucherDetail.DebitAmount) - SUM(VoucherDetail.CreditAmount) as Balance, 
						  Head2.OrderNo as OrderNo 
	               FROM VoucherDetail
                    JOIN Voucher on VoucherDetail.VoucherId = Voucher.VoucherId
                    JOIN HeadGL on VoucherDetail.AccountHeadId = HeadGL.Id
                    JOIN Head5 on HeadGL.ParentId = Head5.Id
                    JOIN Head4 on Head5.ParentId = Head4.Id
                    JOIN Head3 on Head4.ParentId = Head3.Id
                    JOIN Head2 on Head3.ParentId = Head2.Id
                    JOIN Head1 on Head2.ParentId = Head1.Id

                    WHERE Head1.AccCode = 1 
					And Head2.AccCode &lt;&gt; 13 
					and Voucher.CompanyId = @CompanyId
					AND Voucher.IsActive = 1
                    and VoucherDetail.IsActive = 1
                    AND Voucher.IsSubmit =1
                    and Voucher.VoucherDate  &lt;=  @FromDate -- '2021/07/18'
                    GROUP BY Head2.AccName, 
					         Head2.Id, 
							 Head2.OrderNo) AS A
   WHERE A.Balance &lt;&gt; 0
   order by A.OrderNo</CommandText>
      </Query>
      <Fields>
        <Field Name="Id">
          <DataField>Id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="AccName">
          <DataField>AccName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Balance">
          <DataField>Balance</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="OrderNo">
          <DataField>OrderNo</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="CurrentAssetDSet">
      <Query>
        <DataSourceName>BalanceSheetDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StrFromDate">
            <Value>=Parameters!StrFromDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CompanyId">
            <Value>=Parameters!CompanyId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText> DECLARE @FromDate DATE
    SELECT @FromDate = CONVERT(VARCHAR, CONVERT(DATE, @StrFromDate, 105))

	
	SELECT * 
	FROM (SELECT Head4.Id, 
	             Head4.AccName AS AccName, 
				 SUM(VoucherDetail.DebitAmount) - SUM(VoucherDetail.CreditAmount) AS Balance, 
				 Head4.OrderNo AS OrderNo 
	      FROM VoucherDetail
           JOIN Voucher ON VoucherDetail.VoucherId = Voucher.VoucherId
           JOIN HeadGL ON VoucherDetail.AccountHeadId = HeadGL.Id
           JOIN Head5 ON HeadGL.ParentId = Head5.Id
           JOIN Head4 ON Head5.ParentId = Head4.Id
           JOIN Head3 ON Head4.ParentId = Head3.Id
           JOIN Head2 ON Head3.ParentId = Head2.Id
          WHERE Head2.AccCode = '13' 
				AND Head5.AccCode &lt;&gt; '1301005001'
		        AND Voucher.CompanyId =  @CompanyId 
				AND Voucher.IsActive = 1
				AND VoucherDetail.IsActive = 1
                AND Voucher.IsSubmit = 1
                AND Voucher.VoucherDate  &lt;=  @FromDate
          GROUP BY Head4.AccName, Head4.Id, Head4.OrderNo
		  
		  UNION ALL

		  SELECT Head4.Id, 
	             Head4.AccName AS AccName, 
				 SUM(VoucherDetail.DebitAmount)  AS Balance, 
				 Head4.OrderNo AS OrderNo 
	      FROM VoucherDetail
           JOIN Voucher ON VoucherDetail.VoucherId = Voucher.VoucherId
           JOIN HeadGL ON VoucherDetail.AccountHeadId = HeadGL.Id
           JOIN Head5 ON HeadGL.ParentId = Head5.Id
           JOIN Head4 ON Head5.ParentId = Head4.Id
           JOIN Head3 ON Head4.ParentId = Head3.Id
           JOIN Head2 ON Head3.ParentId = Head2.Id
          WHERE  Head5.AccCode = '1301005001'
		        AND Voucher.CompanyId =  @CompanyId 
				AND Voucher.IsActive = 1
				AND VoucherDetail.IsActive = 1
                AND Voucher.IsSubmit = 1
                and Voucher.VoucherDate   between DATEADD(day, -10, @FromDate) and @FromDate
          GROUP BY Head4.AccName, Head4.Id, Head4.OrderNo
		  
		  ) AS A
   WHERE A.Balance &lt;&gt; 0 
   ORDER BY A.OrderNo</CommandText>
      </Query>
      <Fields>
        <Field Name="Id">
          <DataField>Id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="AccName">
          <DataField>AccName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Balance">
          <DataField>Balance</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="OrderNo">
          <DataField>OrderNo</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="CapitalLiabilityDSet">
      <Query>
        <DataSourceName>BalanceSheetDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StrFromDate">
            <Value>=Parameters!StrFromDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CompanyId">
            <Value>=Parameters!CompanyId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>DECLARE @FromDate DATE
    SELECT @FromDate = CONVERT(VARCHAR, CONVERT(DATE, @StrFromDate, 105))   

	
	SELECT * FROM (SELECT Head5.Id, 
	                      Head5.AccName as AccName, 
						  SUM(VoucherDetail.CreditAmount)- SUM(VoucherDetail.DebitAmount) as Balance, 
						  Head5.OrderNo as OrderNo 
	               FROM VoucherDetail
                    JOIN Voucher on VoucherDetail.VoucherId = Voucher.VoucherId
                    JOIN HeadGL on VoucherDetail.AccountHeadId = HeadGL.Id
                    JOIN Head5 on HeadGL.ParentId = Head5.Id
                    JOIN Head4 on Head5.ParentId = Head4.Id
                    JOIN Head3 on Head4.ParentId = Head3.Id
                    JOIN Head2 on Head3.ParentId = Head2.Id
                    JOIN Head1 on Head2.ParentId = Head1.Id

                   WHERE Head1.AccCode = '2' 
				         AND Head2.AccCode &lt;&gt; '24'  --Current Liability
						 AND Head2.AccCode &lt;&gt; '23'  -- Loan A/C
						 AND Voucher.CompanyId =  @CompanyId
						 AND Voucher.IsActive = 1
						 AND VoucherDetail.IsActive = 1
                         AND Voucher.IsSubmit =1
                         AND Voucher.VoucherDate &lt;=  @FromDate
						 --AND (@CostCenterId = 0  or Voucher.Accounting_CostCenterFk = @CostCenterId)
                   GROUP BY Head5.AccName, Head5.Id, Head5.OrderNo
				   UNION All

				   SELECT Head4.Id, 
	                      Head4.AccName as AccName, 
						  SUM(VoucherDetail.CreditAmount)- SUM(VoucherDetail.DebitAmount) as Balance, 
						  Head4.OrderNo as OrderNo 
	               FROM VoucherDetail
                    JOIN Voucher on VoucherDetail.VoucherId = Voucher.VoucherId
                    JOIN HeadGL on VoucherDetail.AccountHeadId = HeadGL.Id
                    JOIN Head5 on HeadGL.ParentId = Head5.Id
                    JOIN Head4 on Head5.ParentId = Head4.Id
                    JOIN Head3 on Head4.ParentId = Head3.Id
                    JOIN Head2 on Head3.ParentId = Head2.Id
                    JOIN Head1 on Head2.ParentId = Head1.Id

                   WHERE Head1.AccCode = '2' 				       
						 AND Head2.AccCode = '23'  -- Loan A/C
						 AND Voucher.CompanyId =  @CompanyId
						 AND Voucher.IsActive = 1
						 AND VoucherDetail.IsActive =1
                         AND Voucher.IsSubmit = 1
                         AND Voucher.VoucherDate &lt;=  @FromDate
						 --AND (@CostCenterId = 0  or Voucher.Accounting_CostCenterFk = @CostCenterId)
                   GROUP BY Head4.AccName, Head4.Id, Head4.OrderNo
				   
				   ) AS A
   WHERE A.Balance &lt;&gt; 0
   ORDER BY A.OrderNo</CommandText>
      </Query>
      <Fields>
        <Field Name="Id">
          <DataField>Id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="AccName">
          <DataField>AccName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Balance">
          <DataField>Balance</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="OrderNo">
          <DataField>OrderNo</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="CurrentLiabilitiesDSet">
      <Query>
        <DataSourceName>BalanceSheetDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StrFromDate">
            <Value>=Parameters!StrFromDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CompanyId">
            <Value>=Parameters!CompanyId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>DECLARE @FromDate DATE
    SELECT @FromDate = CONVERT(VARCHAR, CONVERT(DATE, @StrFromDate, 105)) 

	SELECT * FROM (SELECT Head4.Id, 
	                      Head4.AccName AS AccName, 
						  (SUM(VoucherDetail.CreditAmount) - SUM(VoucherDetail.DebitAmount)) AS Balance, 
						  Head4.OrderNo AS OrderNo 
	               FROM VoucherDetail
                    JOIN Voucher ON VoucherDetail.VoucherId = Voucher.VoucherId
                    JOIN HeadGL ON VoucherDetail.AccountHeadId = HeadGL.Id
                    JOIN Head5 ON HeadGL.ParentId = Head5.Id
                    JOIN Head4 ON Head5.ParentId = Head4.Id
                    JOIN Head3 ON Head4.ParentId = Head3.Id
                    JOIN Head2 ON Head3.ParentId = Head2.Id
                   WHERE Head2.AccCode = '24' 
				         AND Voucher.CompanyId =  @CompanyId
                         AND VoucherDetail.IsActive = 1
						 AND Voucher.IsActive = 1
                         AND Voucher.IsSubmit = 1
                         AND Voucher.VoucherDate &lt;= @FromDate
						-- AND (@CostCenterId = 0  or Voucher.Accounting_CostCenterFk = @CostCenterId)
                   GROUP BY Head4.AccName, Head4.Id, Head4.OrderNo) AS A
   WHERE A.Balance &lt;&gt; 0
   ORDER BY A.OrderNo</CommandText>
      </Query>
      <Fields>
        <Field Name="Id">
          <DataField>Id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="AccName">
          <DataField>AccName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Balance">
          <DataField>Balance</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="OrderNo">
          <DataField>OrderNo</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
