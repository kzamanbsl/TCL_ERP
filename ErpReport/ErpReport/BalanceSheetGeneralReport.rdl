      <Query>
        <DataSourceName>BalanceSheetNewDS</DataSourceName>
        <CommandText>
--declare @StrAsOnDate varchar(10)='31/07/2020'
declare @CompanyId int 
declare @voucherDate date 
--select @voucherDate=convert(varchar, convert(date, @StrAsOnDate, 105))
--Fixed Assets
select 
      h2.AccCode AS ParentCode,UPPER(h2.AccName) AS ParentName,h3.AccCode,h3.AccName,
	  SUM(ISNULL(DebitAmount,0)-ISNULL(CreditAmount,0)) as Balance 
INTO #FixedAssets
from  VoucherDetail vd
inner join Voucher v on v.VoucherId = vd.VoucherId 
inner join HeadGL gl on vd.AccountHeadId = gl.Id
inner join Head5 h5 on gl.ParentId = h5.Id
inner join Head4 h4 on h5.ParentId = h4.Id
inner join Head3 h3 on h4.ParentId = h3.Id
inner join Head2 h2 on h3.ParentId = h2.Id
WHERE h2.CompanyId = @CompanyId and  h2.AccCode like '1%' and LOWER(h2.AccName) &lt;&gt; 'current assets' and
      v.CompanyId = @CompanyId and 
	  vd.IsActive = 1  
      AND v.IsSubmit =1
      AND v.VoucherDate &lt;= @voucherDate
GROUP BY    h2.AccCode,h2.AccName,h3.AccCode, h3.AccName
HAVING      SUM(ISNULL(vd.DebitAmount,0) - ISNULL(vd.CreditAmount,0))&lt;&gt;0
ORDER BY h3.AccCode



SELECT ParentCode,ParentName,SUM(Balance) AS Balance
FROM #FixedAssets
GROUP BY ParentCode,ParentName

DROP Table #FixedAssets</CommandText>
      </Query>
      <Fields>
        <Field Name="ParentCode">
          <DataField>ParentCode</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ParentName">
          <DataField>ParentName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Balance">
          <DataField>Balance</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
      </Fields>
