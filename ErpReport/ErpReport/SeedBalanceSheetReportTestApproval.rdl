    <DataSet Name="ProfitLoss">
      <Query>
        <DataSourceName>BalanceSheetDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StrFromDate">
            <Value>=Parameters!StrFromDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CompanyId">
            <Value>=Parameters!CompanyId.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CostCenterId">
            <Value>=Parameters!CostCenterId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText> DECLARE @FromDate DATE
    SELECT @FromDate = CONVERT(VARCHAR, CONVERT(DATE, @StrFromDate, 105))   

	
	Select 
	(Select SUM(Income)
	From (SELECT 
		(CASE
		WHEN Head1.DrIncrease = 1
			THEN IsNull(CAST(SUM(VoucherDetail.CreditAmount) - SUM(VoucherDetail.DebitAmount) AS decimal(18,2)),0)
			ELSE  IsNull(CAST(SUM(VoucherDetail.DebitAmount) - SUM(VoucherDetail.CreditAmount)AS decimal(18,2)),0)
	END) as Income 
	FROM VoucherDetail
    JOIN Voucher on VoucherDetail.VoucherId = Voucher.VoucherId
    JOIN HeadGL on VoucherDetail.AccountHeadId = HeadGL.Id
    JOIN Head5 on HeadGL.ParentId = Head5.Id
    JOIN Head4 on Head5.ParentId = Head4.Id
    JOIN Head3 on Head4.ParentId = Head3.Id
    JOIN Head2 on Head3.ParentId = Head2.Id
    JOIN Head1 on Head2.ParentId = Head1.Id

 where Head1.AccCode = '3'and Voucher.CompanyId = @CompanyId 
 And Voucher.IsActive = 1 And VoucherDetail.IsActive = 1
 AND Voucher.IsSubmit = 1
 and Voucher.VoucherDate  &lt;=  @FromDate
 AND (@CostCenterId = 0  or Voucher.Accounting_CostCenterFk = @CostCenterId)
 Group By Head1.DrIncrease, HeadGL.Id) AS TotalIncome)

 -

 (Select SUM(Expenses) From 
 (SELECT 
		(CASE
		WHEN Head1.DrIncrease = 1
			THEN IsNull(CAST(SUM(VoucherDetail.CreditAmount) - SUM(VoucherDetail.DebitAmount)AS decimal(18,2)),0)
			ELSE  IsNull(CAST(SUM(VoucherDetail.DebitAmount) - SUM(VoucherDetail.CreditAmount)AS decimal(18,2)),0)
	END) as Expenses 
	FROM VoucherDetail
    JOIN Voucher on VoucherDetail.VoucherId = Voucher.VoucherId
    JOIN HeadGL on VoucherDetail.AccountHeadId = HeadGL.Id
    JOIN Head5 on HeadGL.ParentId = Head5.Id
    JOIN Head4 on Head5.ParentId = Head4.Id
    JOIN Head3 on Head4.ParentId = Head3.Id
    JOIN Head2 on Head3.ParentId = Head2.Id
    JOIN Head1 on Head2.ParentId = Head1.Id

 where Head1.AccCode = '4'and Voucher.CompanyId = @CompanyId 
 And Voucher.IsActive = 1 And VoucherDetail.IsActive = 1
 AND Voucher.IsSubmit =1
 and Voucher.VoucherDate  &lt;=  @FromDate 
AND (@CostCenterId = 0  or Voucher.Accounting_CostCenterFk = @CostCenterId)
 Group By Head1.DrIncrease, HeadGL.Id) AS TotalIncome) AS Balance
	</CommandText>
      </Query>
      <Fields>
        <Field Name="Balance">
          <DataField>Balance</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
