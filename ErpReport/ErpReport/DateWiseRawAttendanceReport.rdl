    <DataSet Name="SummaryDataSet">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StrFromDate">
            <Value>=Parameters!StrFromDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>BEGIN
declare @FromDate date
select @FromDate=convert(varchar, convert(date, @StrFromDate, 105))
DECLARE @atDate VARCHAR(8)= convert(varchar, @FromDate, 112)

  SELECT Count(Distinct emp.EmployeeId) as CountNo,
	d.Name as Department
  FROM Employee as emp
  join Department as d on d.DepartmentId =emp.DepartmentId
  Join Designation as ds on ds.DesignationId = emp.DesignationId
  join RawAttendence as attendence on attendence.EmpCardNo = emp.CardId
  WHERE    attendence.[Date]   = @atDate and emp.Active = 1 and emp.SalaryTag = 1
  GROUP by d.Name
END</CommandText>
      </Query>
      <Fields>
        <Field Name="CountNo">
          <DataField>CountNo</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Department">
          <DataField>Department</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
