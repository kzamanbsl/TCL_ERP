      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>793094b0-b45d-4944-a1b9-fdcb1988db00</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="EmployeeListDSet">
      <Query>
        <DataSourceName>EmployeeListDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Active">
            <Value>=Parameters!Active.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@SalaryTag">
            <Value>=Parameters!SalaryTag.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select    e.EmployeeId as KGID, e.ShortName as NickName,Name,
          (select Url from UrlInfo where UrlType=1)+'Images/Picture/'+(select ImageFileName from Employee where Id=e.Id) as Picture,
		  JoiningDate,(select Name from Designation where DesignationId=e.DesignationId) as Designation,
		  MobileNo,Email,
          (select Name from Employee where Id=e.ManagerId) as ReportingPerson 
from      Employee e
where     Active=@Active and SalaryTag=@SalaryTag
order by  EmployeeId</CommandText>
      </Query>
      <Fields>
        <Field Name="KGID">
          <DataField>KGID</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="NickName">
