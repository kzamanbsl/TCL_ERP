    <DataSet Name="ProfitLossNoteDSet">
      <Query>
        <DataSourceName>ProfitLossNotDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StrFromDate">
            <Value>=Parameters!StrFromDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@StrToDate">
            <Value>=Parameters!StrToDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CompanyId">
            <Value>=Parameters!CompanyId.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CostCenterId">
            <Value>=Parameters!CostCenterId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>declare @FromDate date,@ToDate date,@OpFromDate date,@OpToDate date

select @FromDate = convert(varchar, convert(date, @StrFromDate, 105))
select @ToDate = convert(varchar, convert(date, @StrToDate, 105))
select @OpFromDate = (SELECT DATEADD(month, -1, @FromDate))
select @OpToDate = (SELECT DATEADD(MM,DATEDIFF(MM, -1, @OpFromDate),-1))


SELECT 
  
   1 AS OrderNo,
   Head1.AccCode AS ParantAccCode, 
   Head1.AccName AS ParantAccName, 
   HeadGL.AccCode AS ChildAccCode, 
   HeadGL.AccName AS ChildAccName,
		(CASE
		WHEN Head1.DrIncrease = 1
			THEN IsNull(SUM(VoucherDetail.CreditAmount) - SUM(VoucherDetail.DebitAmount),0)
			ELSE  IsNull(SUM(VoucherDetail.DebitAmount) - SUM(VoucherDetail.CreditAmount),0)
	END) as Balance 
	FROM VoucherDetail
    INNER JOIN Voucher on VoucherDetail.VoucherId = Voucher.VoucherId
    INNER JOIN HeadGL on VoucherDetail.AccountHeadId = HeadGL.Id
    INNER JOIN Head5 on HeadGL.ParentId = Head5.Id
    INNER JOIN Head4 on Head5.ParentId = Head4.Id
    INNER JOIN Head3 on Head4.ParentId = Head3.Id
    INNER JOIN Head2 on Head3.ParentId = Head2.Id
    INNER JOIN Head1 on Head2.ParentId = Head1.Id

 where Head1.AccCode = '3'and Voucher.CompanyId =  @CompanyId 
 AND HeadGL.AccCode &lt;&gt; '4401001001001'
 and Voucher.VoucherDate   between @FromDate And @ToDate
  And Voucher.IsActive = 1 AND VoucherDetail.IsActive = 1
  AND Voucher.IsSubmit = 1
 AND (@CostCenterId = 0  or Voucher.Accounting_CostCenterFk = @CostCenterId)
 Group By Head1.DrIncrease,HeadGL.Id,HeadGL.AccCode,HeadGL.AccName,Head1.Id,Head1.AccCode,Head1.AccName</CommandText>
      </Query>
      <Fields>
        <Field Name="OrderNo">
          <DataField>OrderNo</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="ParantAccCode">
          <DataField>ParantAccCode</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ParantAccName">
          <DataField>ParantAccName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ChildAccCode">
          <DataField>ChildAccCode</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ChildAccName">
          <DataField>ChildAccName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Balance">
          <DataField>Balance</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="EspensessDataSet">
      <Query>
        <DataSourceName>ProfitLossNotDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StrFromDate">
            <Value>=Parameters!StrFromDate.Value</Value>
