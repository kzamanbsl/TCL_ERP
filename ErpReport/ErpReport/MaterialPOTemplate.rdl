  <DataSets>
    <DataSet Name="PurchaseOrderTemplateDSet">
      <Query>
        <DataSourceName>PurchaseOrderTemplateDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@PurchaseOrderId">
            <Value>=Parameters!PurchaseOrderId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select        PO.PurchaseOrderNO as Ref,PO.PurchaseDate,PO.[Days],
              V.Name as Supplier,
              V.ContactName,
			  V.Address as SupplierAddress,
			  V.Phone as SupplierMob,
			  V.Email as SupplierEmail,
			  C.Name as Company,
			  PO.ReferenceNo,
			  (select Name from Employee where Id=PO.EmpId) as ResponsiblePerson,
			  (select Name from Designation where DesignationId=((select DesignationId from Employee where Id=PO.EmpId))) as ResponsiblePersonDesignation
from          Erp.PurchaseOrder PO
INNER JOIN    Erp.Vendor V ON PO.SupplierId=V.VendorId
INNER JOIN    Company C ON PO.CompanyId=C.CompanyId
where         PO.PurchaseOrderId=@PurchaseOrderId</CommandText>
      </Query>
      <Fields>
        <Field Name="Ref">
          <DataField>Ref</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="PurchaseDate">
