  <DataSets>
    <DataSet Name="OrderMapDSet">
      <Query>
        <DataSourceName>OrderMapDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@FromDate">
            <Value>=Parameters!FromDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ToDate">
            <Value>=Parameters!ToDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select om.CustomerId,(select Name from Erp.Vendor where VendorId=om.CustomerId) as Customer,om.Remarks as BillNo,
om.OrderDate,om.OrderNo, od.ProductId,
(select ProductName from Erp.Product where ProductId=od.ProductId) as Product,od.Amount,
om.TmpAgent as Agent,om.TmpCarrying as Carrying, om.TmpNtAmnt NetAmount
from Erp.OrderDetail od
inner join Erp.OrderMaster  om on om.OrderMasterId=od.OrderMasterId
where om.OrderDate&gt;=@FromDate and om.OrderDate&lt;=@ToDate
order by om.OrderDate,Product</CommandText>
      </Query>
