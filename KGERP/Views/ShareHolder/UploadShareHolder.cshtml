@model KGERP.Service.ServiceModel.ShareHolderModel
@using KGERP.Utility
@{
    ViewBag.Title = "Create";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


@using (Html.BeginForm("UploadShareHolder", "ShareHolder", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    @Html.HiddenFor(x => x.CompanyId)
    <table class="table">

        <tr>
            <th>Browse File</th>
            <td>
                <input type="file" name="UploadedFile" />
            </td>
        </tr>

        <tr>
            <td colspan="2">
                <input type="submit" name="Submit" value="Submit" />
            </td>
        </tr>

    </table>
}


@section Scripts {
    @Scripts.Render("~/bundles/jqueryui")
    @Styles.Render("~/Content/cssjqryUi")

    <script type="text/javascript">

    </script>

}
